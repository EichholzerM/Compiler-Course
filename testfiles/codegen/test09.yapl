/* CodeGen test input - WhileStatement.
 * $Id: test09.yapl 115 2009-06-23 14:51:36Z mt $
 */
Program test09
Declare
    Const n = 7;
    k: Integer;
Begin
    Write "--- Start test09 ---";
    writeln();
    k := 0;
    While k < n Do
        k := k+1;
        Write "k = ";
        writeint(k);
        writeln();
    EndWhile;
    Write "(7 iterations expected).";
    writeln();
    Write "--- End test09 ---";
    writeln();
End test09.
