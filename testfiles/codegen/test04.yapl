/* CodeGen test input - simple assignments to global variables.
 * $Id: test04.yapl 115 2009-06-23 14:51:36Z mt $
 */
Program test04
Declare
	Const constant = 10;
	k: Integer;
	b: Boolean;
Begin
    Write "--- Start test04 ---";
    writeln();
    k := 3;
    b := True;
	Write "writeint(k): ";
	writeint(k);
	Write " (expected 3)";
	writeln();
    k := constant;
	Write "writeint(k): ";
	writeint(k);
	Write " (expected 10)";
	writeln();
	Write "writebool(b): ";
	writebool(b);
	Write " (expected True)";
	writeln();
    Write "--- End test04 ---";
    writeln();
End test04.
