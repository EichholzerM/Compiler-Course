 */
Program test15

/* Print value if value >= threshold. */
Procedure printIF(value, threshold: Integer)
Begin
    If value < threshold Then
        Return;
    EndIf;
    writeint(value);
    Write " ";
End printIF;

Procedure print(val : Integer)
Begin
	writeint(val);
	Write ".";
	writeln();
End print;

Procedure square(x: Integer): Integer
Begin
    Return x*x;
End square;

Declare
    Const N = 8;
    k, t: Integer;
Begin
    k := 1;
    While k < 5 Do
        print(square(k));
        k := k+1;
    EndWhile;
End test15.
